@model Survey
<head>
    <title>Dojo Survey Index</title>
</head>
<body>
    <div class="container">
        <h2>Submitted Info</h2>
        <div>
            @{
                ViewData["title"] = "Dojo Survey Index";
            }
            <p>Name: @Model.Name</p>
        </div>
        <div>
            <p>Dojo Location: @Model.Location</p>
        </div>
        <div>
            <p>Favorite Language: @Model.Language</p>
        </div>
        <div>
            <p>Comments: @Model.Comment</p>
        </div>
        <a href="/" class="btn btn-warning">Go Back</a>
    </div>
</body>